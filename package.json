{
  "name": "shopping-cart",
  "version": "0.9.0",
  "description": "Interactive, customisable shopping cart with discounts computation functionality",
  "main": "src/index.js",
  "author": "Pablo Deeleman <deeleman@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:parcel\"",
    "dev:api": "node api-dev-server.js",
    "dev:parcel": "parcel src/index.html -p 9000",
    "build": "rimraf ./dist && parcel build src/index.html --no-cache",
    "test": "jest --silent",
    "lint": "eslint src/**/*.ts --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "promise-polyfill": "8.1.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "shopping-cart": "^1.0.4",
    "whatwg-fetch": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "babel-jest": "26.1.0",
    "babel-polyfill": "6.26.0",
    "concurrently": "5.2.0",
    "cors": "2.8.5",
    "eslint": "7.4.0",
    "eslint-plugin-html": "6.0.2",
    "express": "4.17.1",
    "jest": "26.1.0",
    "parcel-bundler": "1.12.4",
    "rimraf": "3.0.2",
    "sass": "^1.26.10",
    "typescript": "3.9.6"
  }
}
